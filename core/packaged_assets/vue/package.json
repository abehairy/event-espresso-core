{
  "name": "ee-vue",
  "version": "1.0.0",
  "description": "Models component for implementation with vue.",
  "main": "src/ee-vue-models.js",
  "watch": {
    "{src,test}/*.js": [
      "dev"
    ]
  },
  "scripts": {
    "dev": "watchify src/ee-vue-models.js -o dist/ee-vue-models.build.js -vd --poll=100 -t aliasify",
    "watch": "npm-scripts-watcher",
    "build": "cross-env NODE_ENV=production browserify -t aliasify src/ee-vue-models.js | uglifyjs -c warnings=false -m > dist/ee-vue-models.build.js",
    "alias": "aliasify --help"
  },
  "author": "Darren Ethier",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.1.4",
    "vue-resource": "^1.0.3"
  },
  "aliasify": {
    "aliases" : {
      "vue": "vue/dist/vue.common.js"
    }
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.3",
    "uglify-js": "^2.7.5",
    "vue": "^2.1.4",
    "vue-resource": "^1.0.3",
    "vueify": "^9.3.0",
    "vuex": "^2.0.0",
    "watchify": "^3.7.0"
  }
}
